<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="15" alignment="CENTER" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.playops.controllers.LoginController" prefWidth="400" prefHeight="250">
    <Label text="PlayOps Store Login" style="-fx-font-size: 20px;"/>
    <TextField fx:id="emailField" promptText="Email"/>
    <Button text="Login" fx:id="loginButton" onAction="#handleLogin"/>
</VBox>
